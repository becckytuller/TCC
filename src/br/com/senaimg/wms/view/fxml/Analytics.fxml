<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="rootTabPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.senaimg.wms.view.controller.AnalyticsController">
    <stylesheets>
        <URL value="@/br/com/senaimg/wms/view/stylesheet/analytics.css" />
    </stylesheets>
   <children>
      <FlowPane alignment="CENTER" hgap="10.0" vgap="10.0">
         <children>
            <Button fx:id="stockReport" contentDisplay="TOP" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#stockReportHandle" prefHeight="190.0" prefWidth="190.0" text="Inventory Report" textAlignment="CENTER" wrapText="true">
               <graphic>
                  <ImageView fitHeight="134.0" fitWidth="134.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/funcStock.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="minimumStockReport" contentDisplay="TOP" layoutX="10.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#minimumStockReportHandle" prefHeight="190.0" prefWidth="190.0" text="Minimum Inventory Report" textAlignment="CENTER" wrapText="true">
               <graphic>
                  <ImageView fitHeight="134.0" fitWidth="134.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/funcStock.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="supplierReport" contentDisplay="TOP" layoutX="210.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#supplierReportHandle" prefHeight="190.0" prefWidth="190.0" text="Suppliers Report" textAlignment="CENTER" wrapText="true">
               <graphic>
                  <ImageView fitHeight="134.0" fitWidth="134.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/funcSupplier.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button fx:id="customerReport" contentDisplay="TOP" layoutX="410.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#customerReportHandle" prefHeight="190.0" prefWidth="190.0" text="Customer Report" textAlignment="CENTER" wrapText="true">
               <graphic>
                  <ImageView fitHeight="134.0" fitWidth="134.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../images/funcCustomer.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
         </children>
         <StackPane.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </StackPane.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </FlowPane>
   </children>
</StackPane>
